{
  "name": "@youcraft/recall-embeddings-openai",
  "version": "0.1.0",
  "description": "OpenAI embeddings provider for Recall. Generate text embeddings using text-embedding-3-small, text-embedding-3-large, and other OpenAI models.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@youcraft/recall": "workspace:*",
    "openai": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "recall",
    "ai",
    "memory",
    "llm",
    "embeddings",
    "openai",
    "text-embedding",
    "vector-database",
    "semantic-search",
    "typescript"
  ],
  "license": "MIT",
  "homepage": "https://github.com/aneequrrehman/recall#readme",
  "bugs": {
    "url": "https://github.com/aneequrrehman/recall/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneequrrehman/recall.git",
    "directory": "packages/embeddings-openai"
  }
}
