{
  "name": "@youcraft/recall-extractor-anthropic",
  "version": "0.2.0-alpha.0",
  "description": "Anthropic-powered memory extraction for Recall. Extract facts from conversations using Claude models with smart deduplication and consolidation.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
