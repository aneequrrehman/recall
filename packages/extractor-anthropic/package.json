    "vitest": "^2.0.0"
  },
  "keywords": [
    "recall",
    "ai",
    "memory",
    "llm",
    "extraction",
    "anthropic",
    "claude",
    "fact-extraction",
    "conversational-ai",
    "chatbot",
    "typescript"
  ],
